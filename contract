POST /suggestions/organizations
{
  query: number;
}

Response:
{
  id: number;
  name: string;
}[];

POST /suggestions/workspaces
{
  organization_id: number;
  query: number;
}

Response:
{
  id: number;
  name: string;
}[];

POST /suggestions/prompts
{
  query: string;
}

Response:
{
  propmpts: string[];
}

POST /message
{
  organization?: number;
  wokspace_id?: number;
  industry?: string;
  propmpt: string;
}

Response:
{
  key: string;
  name: string;
  value: string | number;
}[];